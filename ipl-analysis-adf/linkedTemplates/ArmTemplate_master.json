{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ipl-analysis-adf"
		},
		"ipl_load_pipeline_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0915-191217-jckbtnyt"
		},
		"ipl_trigger_properties_pipeline_parameters_adf_year": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ipl-analysis-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ipl_load_pipeline_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ipl_load_pipeline_properties_typeProperties_existingClusterId')]"
					},
					"ipl_trigger_properties_pipeline_parameters_adf_year": {
						"value": "[parameters('ipl_trigger_properties_pipeline_parameters_adf_year')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}